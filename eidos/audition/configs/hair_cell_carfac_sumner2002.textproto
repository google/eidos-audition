# Hair cell model by Meddis fed by CARFAC BM model.

config {
    audio_scaling_gain: 0.095
}

pipeline {
    models {  # Use CARFAC as BM model here.
        model_type: MODEL_CARFAC
        stage_type: STAGE_HAIR_CELLS
        config {
            [type.googleapis.com/eidos.audition.CarfacConfig] {
                output_types {
                    store_nap: false
                    store_bm: true
                }
            }
        }
    }
    models {
        model_type: MODEL_SUMNER_SYNAPSE_2002
        stage_type: STAGE_HAIR_CELLS
        config {
            [type.googleapis.com/eidos.audition.Sumner2002HairCellSynapseConfig] {
                ca_cond_mode: SUMNER2002_CA_COND_MODE_ORIGINAL
                output_spikes: false
            }
        }
    }
}

